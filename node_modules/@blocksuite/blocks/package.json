{
  "name": "@blocksuite/blocks",
  "version": "0.6.0",
  "description": "Default BlockSuite editable blocks.",
  "main": "dist/index.js",
  "type": "module",
  "repository": "toeverything/blocksuite",
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "peerDependencies": {
    "yjs": "^13",
    "@blocksuite/lit": "0.6.0",
    "@blocksuite/store": "0.6.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "hotkeys-js": "^3.10.1",
    "html-to-image": "^1.11.11",
    "lit": "^2.7.3",
    "marked": "^4.2.12",
    "shiki": "^0.14.1",
    "turndown": "^7.1.1",
    "zod": "^3.21.4",
    "jszip": "^3.10.1",
    "@blocksuite/connector": "0.6.0",
    "@blocksuite/global": "0.6.0",
    "@blocksuite/phasor": "0.6.0",
    "@blocksuite/virgo": "0.6.0"
  },
  "exports": {
    "./dist/*": "./dist/*",
    "./src/*": "./dist/*.js",
    ".": {
      "module": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./std": {
      "types": "./dist/std.d.ts",
      "default": "./dist/std.js"
    },
    "./models": {
      "types": "./dist/models.d.ts",
      "default": "./dist/models.js"
    },
    "./content-parser": {
      "types": "./dist/content-parser.d.ts",
      "default": "./dist/content-parser.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/autosize": "^4.0.1",
    "@types/marked": "^4.0.8",
    "@types/turndown": "^5.0.1",
    "yjs": "^13.6.0",
    "@blocksuite/store": "0.6.0"
  },
  "scripts": {
    "build": "tsc",
    "test:unit": "vitest --run",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test": "pnpm test:unit && pnpm test:e2e"
  },
  "types": "dist/index.d.ts"
}